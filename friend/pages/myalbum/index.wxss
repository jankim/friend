/* pages/gallery/index.wxss */
@import '../../utils/wux-components/wux.wxss'; 

.weui-cell {
  padding: 40rpx 18rpx 40rpx 30rpx;
  position: relative;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.weui-cell:before {
  content: " ";
  position: absolute;
  left: 0;
  top: 0;
  right: 0;
  height: 1px;
  color: #D9D9D9;
  left: 15rpx;
}

.weui-cells {
  position: relative;
  margin-top: 1.17647059em;
  background-color: #FFFFFF;
  line-height: 1.41176471;
  font-size: 17rpx;
}
.weui-cells:before {
  content: " ";
  position: absolute;
  left: 0;
  top: 0;
  right: 0;
  height: 1px;
  color: #D9D9D9;
}
.weui-cells:after {
  content: " ";
  position: absolute;
  left: 0;
  bottom: 0;
  right: 0;
  height: 1px;
  color: #D9D9D9;
}

.weui-cells_after-title {
  margin-top: 0;
}
.weui-cell__bd {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
}
.weui-uploader__file {
  float: left;
  margin-right: 12rpx;
  margin-bottom: 12rpx;
}
.weui-uploader__img {
  display: block;
  width: 222rpx;
  height: 222rpx;
  border-radius: 5px;
}
