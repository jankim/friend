.container {
  padding-top: 40rpx;
  background: #fff;
}


.tapbar{
	background-color: #fff;
  margin-left:30rpx;
  border-bottom: 1px solid #efefef
}


.cell{
  display: flex;
  display: -webkit-flex;
  flex-direction: row;
  height: 90rpx;
  line-height: 90rpx;
  color: #000;
}
.cell .left{
  width: 23%;
}
.cell .go{
  width: 550rpx;
  display: flex;
  display: -webkit-flex;
  justify-content:center;
  flex-direction: column;
  color: #cccccc;
}
.btn-area{margin-top: 70rpx;}
.phcolor{color: #cccccc;font-size: 10pt}