
@font-face {font-family: "iconfont";
  src: url('data:application/x-font-woff;charset=utf-8;base64,') format('woff');
}

.iconfont {
  font-family:"iconfont" !important;
  font-style:normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-xingbienan:before { content: "\e657"; }

.icon-xingbienv:before { content: "\e656"; }


.container {
  background: #fff;
}

.tapbar{
  margin-top: 20rpx;
	background-color: #fff;
	padding-left: 30rpx;
  display: flex;
  display: -webkit-flex;
  flex: 1;
}
.tapbar .left{
  flex: 0 0 100rpx;
  order: -1;
}
.tapbar .middle{
  flex: 1;
}
.tapbar .middle .userinfo{
  display: flex;
  display: -webkit-flex;
  align-items:center ;
  font-size:38rpx; 
}
.female .iconfont {
  padding-right:7rpx;
  font-size:20rpx;
}
.male .iconfont {
  padding-right:7rpx;
  font-size:28rpx;
}
.tapbar .middle .userinfo .name{
  font-size: 32rpx;
  max-width: 330rpx;
  overflow:hidden; 
  text-overflow:ellipsis;
  white-space:nowrap;
}
.female{
  background: #ffb8d3;
}
.male{
  background: #81d4fa;
}
.tapbar .middle .userinfo .gender{
  display:flex;
  align-items:center;
  margin-left:10rpx;
  padding:0rpx 8rpx;
  border-radius:10rpx;
  color: #fff;
  font-size:26rpx;
}
.tapbar .right{
  display: flex;
  display: -webkit-flex;
  align-items:center;
  justify-content:flex-end;
  padding-right:30rpx;
  flex: 0 0 142rpx;
}
.white{
  color: white;
}
.capsule{
  padding: 8rpx 18rpx 8rpx 20rpx;
  border-radius: 10rpx;
  font-size:22rpx;
  letter-spacing:3rpx;
}
.focus{
  border: 1rpx solid #cccccc;
  color: #cccccc;
}
.no-focus{
  border: 1rpx solid #0092ff;
  color: #0092ff;
}

.h-130{
  height: 130rpx;
}

.user-info{
	background-color: #fff;
  height: 180rpx;
}
.user-info image{
	display: block;
	width: 130rpx;
	height: 80rpx;
	border-radius: 50%;
	box-shadow: 0 0 20rpx rgba(0,0,0,.2)
}
.user-info .user-avatar{
  width: 80rpx;
  height: 130rpx;
  float: left;
  left: 30rpx;
  display: flex;
  display: -webkit-flex;
  justify-content:center;
  align-items:center;
}




.blue{
  color: #0092ff
}
.border-bottom{
  border-bottom:1px solid #f5f5f5;
}
